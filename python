class students:
    
    def _init_(self,name,gender,city,DOB,phoneno):
        self.name= name
        self.gender=gender
        self.city=city
        self.DOB.DOB
        self.phoneno=phoneno
        
    def address(self):
        addr=f"Name : {self.name}\nGender : {self.gender}\ncity : {self.city}\nDOB : {self.DOB}\nPhoneno : {self. pheneno}"
        return addr
        
    def age(self)
        current_year = 2022
        return current_year -self.DOB
        
